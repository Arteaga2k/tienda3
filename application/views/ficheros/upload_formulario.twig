{% extends "_templates/base.twig" %} {% block contenido %}


<div class="col-md-12">
	{% if form.error|length %}
	   <div class="alert alert-danger" role="alert">{{ form.error|raw }}</div>
	{% endif %}
	<div class="col-md-5">

		<h3>Importar archivos</h3>
		<hr />
		{{ form.form_upload|raw }}
		<div
			class="form-group {% if form.username|length -%} has-error {%- endif -%}">

			<label for="username" class="col-sm-2 control-label">Username</label>
			<div class="col-sm-10">
				<input type="file" class="form-control" id="filename"
					name="filename" value="{{ set_value('filename') }}"> <span>{{
					form.filename|raw }}</span>
			</div>
		</div>	
		
		<div class="form-group">
			<div class="col-sm-offset-2 col-sm-10">
				<button type="submit" class="btn btn-primary">Subir</button>
			</div>
		</div>
		</form>
	</div>
	{% endblock %}